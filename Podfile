  pod 'Moya/RxSwift'
#  pod 'RxSwift'
#  pod 'RxCocoa'
  pod 'PromiseKit'
  pod 'MJRefresh'
  pod 'HanekeSwift' , :git => 'https://github.com/Haneke/HanekeSwift.git', :branch => 'feature/swift-3'
  
  pod 'DATAStack', '~> 6'
  pod 'QueryKit'
end
